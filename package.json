{
  "name": "javascript-spa",
  "version": "1.0.0",
  "description": "My own made JavaScript SPA application scaffolding",
  "scripts": {
    "build:dev": "webpack --env dev",
    "build:watch": "webpack --env dev --watch",
    "build:prod": "webpack --env prod",
    "start:dev": "webpack-dev-server --hot --inline --env dev",
    "start:prod": "npm run build:prod && http-server ./dist"
  },
  "keywords": [
    "spa",
    "javascript"
  ],
  "author": "Steven Liebregt <stevenliebregt@outlook.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "http-server": "^0.11.1"
